{
  "name": "harvest-report-lambda",
  "version": "1.0.0",
  "description": "",
  "main": "handler.js",
  "scripts": {
    "lint": "prettier **/!(*.js) --check --loglevel warn && eslint .",
    "lint:fix": "prettier **/*.* --write --loglevel warn && eslint . --fix",
    "pretest": "npm run lint",
    "test": "jest --coverage",
    "start": "serverless offline start",
    "ci": "npm run test && npm run deploy",
    "deploy": "npm run deploy:dev && npm run deploy:prod",
    "deploy:dev": "serverless deploy -v",
    "deploy:prod": "serverless deploy -v --stage prod",
    "remove-deploys": "serverless remove -v && serverless remove -v --stage prod"
  },
  "author": "Daniel Edholm Ignat",
  "license": "MIT",
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "devDependencies": {
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.1",
    "jest-when": "^3.0.0",
    "mockdate": "^3.0.2",
    "prettier": "2.1.2",
    "serverless": "^2.9.0",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-offline": "^6.8.0"
  },
  "dependencies": {
    "date-fns": "^2.16.1",
    "dinero.js": "^1.8.1",
    "harvest": "^2.2.5",
    "json2csv": "^4.5.4",
    "lodash.groupby": "^4.6.0",
    "lodash.mapvalues": "^4.6.0"
  }
}
