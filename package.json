{
  "name": "harvest-report-lambda",
  "version": "1.0.0",
  "description": "",
  "main": "handler.js",
  "scripts": {
    "lint": "prettier \"**/!(*.js)\" --check --loglevel warn && eslint .",
    "lint:fix": "prettier \"**/*.*\" --write --loglevel warn && eslint . --fix",
    "pretest": "npm run lint",
    "test": "jest --coverage",
    "local-invocation": "serverless invoke local --function hours",
    "start": "serverless offline start",
    "deploy": "npm run deploy:dev && npm run deploy:prod",
    "deploy:dev": "serverless deploy -v",
    "deploy:prod": "serverless deploy -v --stage prod",
    "remove-deploys": "serverless remove -v && serverless remove -v --stage prod",
    "check-all-pull-request-commits": "git rebase origin/main --exec \"npm run test && npm run local-invocation\""
  },
  "author": "Daniel Edholm Ignat",
  "license": "MIT",
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-tests.js"
    ]
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "mockdate": "^3.0.5",
    "msw": "^0.28.2",
    "prettier": "2.2.1",
    "serverless": "^2.38.0",
    "serverless-offline": "^6.9.0",
    "serverless-plugin-typescript": "^1.1.9",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "date-fns": "^2.21.1",
    "dinero.js": "^1.8.1",
    "lodash.groupby": "^4.6.0",
    "lodash.mapvalues": "^4.6.0",
    "node-fetch": "^2.6.1"
  }
}
